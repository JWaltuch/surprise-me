{
  "name": "surprise-me",
  "version": "2.0.0",
  "description": "Wish List App",
  "type": "module",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token.js",
    "lint": "eslint ./client ./script ./server --fix",
    "lint-fix": "npm run lint --fix",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require core-js/stable --require regenerator-runtime/runtime --require @babel/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "assert": "^2.1.0",
    "axios": "^1.7.9",
    "browserify-zlib": "^0.2.0",
    "compression": "^1.7.3",
    "core-js": "^3.39.0",
    "crypto-browserify": "^3.12.1",
    "eslint-plugin-prettier": "^5.2.1",
    "events": "^3.3.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.1",
    "firebase-functions": "^6.1.2",
    "firebase-tools": "^13.29.1",
    "history": "^5.3.0",
    "https-browserify": "^1.0.0",
    "morgan": "^1.9.1",
    "net": "^1.0.2",
    "node": "^23.4.0",
    "os-browserify": "^0.3.0",
    "passport-google-oauth": "^2.0.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "querystring-es3": "^0.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.14.1",
    "request": "^2.88.0",
    "semver": "^7.6.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tls": "0.0.1",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.25.9",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-throw-expressions": "^7.25.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.4.0",
    "ajv": "^8.17.1",
    "axios-mock-adapter": "^2.1.0",
    "babel-loader": "^9.2.1",
    "chai": "^5.1.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^9.17.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.37.2",
    "git-url-parse": "^16.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^15.2.11",
    "mocha": "^11.0.2",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "nodemon": "^3.1.9",
    "npm-merge-driver": "^2.3.6",
    "prettier": "^3.4.2",
    "react-test-renderer": "^16.4.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "simple-git": "^3.27.0",
    "supertest": "^7.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "yaml": "^1.6.0"
  }
}
